{
  "name": "atlas-chat-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@sentry/react": "^9.12.0",
    "@sentry/tracing": "^7.120.3",
    "@tanstack/react-query": "^4.0.0",
    "librechat-data-provider": "file:../packages/data-provider",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "overrides": {
    "@radix-ui/react-icons": "1.3.0"
  },
  "resolutions": {
    "@radix-ui/react-icons": "1.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^5.11.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "start": "craco start",
    "analyze:source-map": "source-map-explorer 'build/static/js/*.js'",
    "analyze:webpack": "webpack-bundle-analyzer build/static/js/*.js",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --no-cache 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "NODE_OPTIONS=--max-old-space-size=4096 eslint --no-cache 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md}' --config ./.prettierrc.cjs"
  }
}
